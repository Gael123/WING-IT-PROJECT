<div style="background: #E67E22;">
  <div class = "dashboard container">
    <div class ="dashboard" style ="color: black">
      <h1 class = "dashboard-header" style ="color:#6c757d"> Welcome to <strong><%= current_user.username %> !</strong>  </h1>
      <div class = "post_container">
        <div class = posts-list>
          <h2 class = "mb-3" style ="color:#1EDD88"> These Are Your posts <%= current_user.username %></h2>
          <p><% current_user.posts.each do |post| %></p>
            <%= cl_image_tag post.photo, height: 50, width: 50, crop: :fill %>
            <%= post.content %>
            <p>Upvotes: <%= post.get_upvotes.size %></p>
          <p><%= post.created_at.strftime("Posted on: %m/%d/%Y")%></p>
        <% end %>
        </div>
        <div class = "bookmark-list">
          <h2 class = "mb-3" style ="color:#1EDD88">BookMarks</h1>
    </div>
    </div>
  </div>
</div>






